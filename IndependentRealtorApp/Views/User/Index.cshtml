@model IEnumerable<IndependentRealtorApp.Models.DomainModels.User>

@{
    ViewData["Title"] = "User List Page";
}
<div class="bg-muted text-dark text-center p-1">
    <h1 class="m-2 p-3">Temp layout for User for Dependency Injection - Unit Testing</h1>
</div>
<table class="table table-bordered table-striped mt-2 less-transparent-table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Full Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.UserEmail</td>
                <td>@user.FullName</td>
            </tr>
        }
    </tbody>
</table>